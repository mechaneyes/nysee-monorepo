{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_WORDPRESS_API_URL",
        "WORDPRESS_API_URL",
        "WORDPRESS_URL",
        "WORDPRESS_HOSTNAME"
      ],
      "outputs": ["dist/**"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_WORDPRESS_API_URL",
        "WORDPRESS_API_URL",
        "WORDPRESS_URL",
        "WORDPRESS_HOSTNAME"
      ],
      "outputs": [".next/**", "!.next/cache/**"]
    }
  },
  "globalEnv": [
    "GITHUB_TOKEN",
    "NEXT_PUBLIC_WORDPRESS_API_URL",
    "WORDPRESS_API_URL",
    "WORDPRESS_URL",
    "WORDPRESS_HOSTNAME"
  ],
  "globalDependencies": [
    "tsconfig.json"
  ]
}